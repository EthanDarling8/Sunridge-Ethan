@page
@inject UserManager<Owner> UserManager
@inject SignInManager<Owner> SignInManager
@using Sunridge.Models
@using Microsoft.AspNetCore.Identity
@model Sunridge.Pages.Admin.Lot.Index

@{
    ViewData["Title"] = "Lot";
}

<div class="container">
    <h1>Lots</h1>
    <table id="DT_load" class="table table-striped table-bordered">
        <a asp-page="Upsert" class="btn btn-primary mt-4 mb-3"><i class="fas fa-plus"></i>&nbsp;Create New</a>
        <thead>
            <tr>
                <th>
                    Lot Number
                </th>
                <th>
                    Address
                </th>
                <th>
                    Owners
                </th>
                <th>
                    Tax Id
                </th>
                <th>
                    Inventory
                </th>
                <th>
                </th>
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
</div>

@section Scripts{
    <script src="~/js/lot.js"></script>
}
